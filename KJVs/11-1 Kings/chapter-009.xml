			<phrase strongs="5414">had given</phrase>
			<phrase strongs="3474">him; and they pleased</phrase>
			<phrase strongs="5869">him not.</phrase>
		</verse>
		<verse num="13">
			<phrase strongs="559">And he said,</phrase>
			<phrase strongs="5892">What cities</phrase>
			<phrase strongs="5414">are these which thou hast given</phrase>
			<phrase strongs="251">me, my brother?</phrase>
			<phrase strongs="7121">And he called</phrase>
			<phrase strongs="776">them the land</phrase>
			<phrase strongs="3521">of Cabul</phrase>
			<phrase strongs="3117">unto this day.</phrase>
		</verse>
		<verse num="14">
			<phrase strongs="2438">And Hiram</phrase>
			<phrase strongs="7971">sent</phrase>
			<phrase strongs="4428">to the king</phrase>
			<phrase strongs="3967">sixscore</phrase>
			<phrase strongs="6242"></phrase>
			<phrase strongs="3603">talents</phrase>
			<phrase strongs="2091">of gold.</phrase>
		</verse>
		<verse num="15">
