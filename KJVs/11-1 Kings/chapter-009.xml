			<phrase strongs="8010">Solomon</phrase>
			<phrase strongs="730">with cedar</phrase>
			<phrase strongs="6086">trees</phrase>
			<phrase strongs="1265">and fir</phrase>
			<phrase strongs="6086">trees,</phrase>
			<phrase strongs="2091">and with gold,</phrase>
			<phrase strongs="2656">according to all his desire</phrase>
			<phrase strongs="227">,) that then</phrase>
			<phrase strongs="4428">king</phrase>
			<phrase strongs="8010">Solomon</phrase>
			<phrase strongs="5414">gave</phrase>
			<phrase strongs="2438">Hiram</phrase>
			<phrase strongs="6242">twenty</phrase>
			<phrase strongs="5892">cities</phrase>
			<phrase strongs="776">in the land</phrase>
