			<phrase strongs="559">and said,</phrase>
			<phrase strongs="7586">Saul</phrase>
			<phrase strongs="5221">hath slain</phrase>
			<phrase strongs="505">his thousands,</phrase>
			<phrase strongs="1732">and David</phrase>
			<phrase strongs="7233">his ten thousands.</phrase>
		</verse>
		<verse num="8">
