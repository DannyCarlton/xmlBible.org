			<phrase strongs="7586">Saul,</phrase>
			<phrase strongs="1697">and the thing</phrase>
			<phrase strongs="3474">pleased</phrase>
			<phrase strongs="5869">him.</phrase>
		</verse>
		<verse num="21">
			<phrase strongs="7586">And Saul</phrase>
			<phrase strongs="559">said,</phrase>
			<phrase strongs="5414">I will give</phrase>
			<phrase strongs="4170">him her, that she may be a snare</phrase>
			<phrase strongs="3027">to him, and that the hand</phrase>
			<phrase strongs="6430">of the Philistines</phrase>
			<phrase strongs="7586">may be against him. Wherefore Saul</phrase>
			<phrase strongs="559">said</phrase>
			<phrase strongs="1732">to David,</phrase>
			<phrase strongs="3117">Thou shalt this day</phrase>
			<phrase strongs="2859">be my son in law</phrase>
			<phrase strongs="8147">in the one of the twain.</phrase>
		</verse>
		<verse num="22">
			<phrase strongs="7586">And Saul</phrase>
