	<chapter num="18">
		<verse num="1">
			<phrase strongs="3615">And it came to pass, when he had made an end</phrase>
			<phrase strongs="1696">of speaking</phrase>
			<phrase strongs="7586">unto Saul,</phrase>
			<phrase strongs="5315">that the soul</phrase>
			<phrase strongs="3083">of Jonathan</phrase>
			<phrase strongs="7194">was knit</phrase>
			<phrase strongs="5315">with the soul</phrase>
			<phrase strongs="1732">of David,</phrase>
			<phrase strongs="3083">and Jonathan</phrase>
			<phrase strongs="157">loved</phrase>
			<phrase strongs="5315">him as his own soul.</phrase>
		</verse>
		<verse num="2">
			<phrase strongs="7586">And Saul</phrase>
			<phrase strongs="3947">took</phrase>
			<phrase strongs="3117">him that day,</phrase>
			<phrase strongs="5414">and would let</phrase>
			<phrase strongs="7725">him go no more home</phrase>
			<phrase strongs="1">to his father's</phrase>
			<phrase strongs="1004">house.</phrase>
		</verse>
