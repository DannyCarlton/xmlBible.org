		</verse>
		<verse num="10">
			<phrase strongs="582">Then were the men</phrase>
			<phrase strongs="1419">exceedingly</phrase>
			<phrase strongs="3374"></phrase>
			<phrase strongs="3372">afraid,</phrase>
			<phrase strongs="559">and said</phrase>
