<book name="Job" id="18">
	<chapter num="25">
		<verse num="1">
			<phrase strongs="6030">Then answered</phrase>
			<phrase strongs="1085">Bildad</phrase>
			<phrase strongs="7747">the Shuhite,</phrase>
			<phrase strongs="559">and said,</phrase>
		</verse>
		<verse num="2">
			<phrase strongs="4910">Dominion</phrase>
			<phrase strongs="6343">and fear</phrase>
			<phrase strongs="6213">are with him, he maketh</phrase>
			<phrase strongs="7965">peace</phrase>
			<phrase strongs="4791">in his high places.</phrase>
		</verse>
		<verse num="3">
			<phrase strongs="3426">Is there</phrase>
			<phrase strongs="4557">any number</phrase>
			<phrase strongs="1416">of his armies?</phrase>
			<phrase strongs="216">and upon whom doth not his light</phrase>
			<phrase strongs="6965">arise?</phrase>
		</verse>
		<verse num="4">
			<phrase strongs="582">How then can man</phrase>
			<phrase strongs="6663">be justified</phrase>
			<phrase strongs="410">with God?</phrase>
			<phrase strongs="2135">or how can he be clean</phrase>
			<phrase strongs="3205">that is born</phrase>
			<phrase strongs="802">of a woman?</phrase>
		</verse>
		<verse num="5">
			<phrase strongs="3394">Behold even to the moon,</phrase>
			<phrase strongs="166">and it shineth</phrase>
			<phrase strongs="3556">not; yea, the stars</phrase>
			<phrase strongs="2141">are not pure</phrase>
			<phrase strongs="5869">in his sight.</phrase>
		</verse>
		<verse num="6">
			<phrase strongs="582">How much less man,</phrase>
			<phrase strongs="7415">that is a worm?</phrase>
			<phrase strongs="1121">and the son</phrase>
			<phrase strongs="120">of man,</phrase>
			<phrase strongs="8438">which is a worm?</phrase>
		</verse>
