<book name="Psalms" id="19">
	<chapter num="108">
		<verse num="1">
			<phrase strongs="7892">A Song</phrase>
			<phrase strongs="4210">or Psalm</phrase>
			<phrase strongs="1732">of David.</phrase>
			<phrase strongs="430">O God,</phrase>
			<phrase strongs="3820">my heart</phrase>
			<phrase strongs="3559">is fixed;</phrase>
			<phrase strongs="7891">I will sing</phrase>
			<phrase strongs="2167">and give praise,</phrase>
			<phrase strongs="3519">even with my glory.</phrase>
		</verse>
		<verse num="2">
			<phrase strongs="5782">Awake,</phrase>
			<phrase strongs="5035">psaltery</phrase>
			<phrase strongs="3658">and harp:</phrase>
			<phrase strongs="5782">I myself will awake</phrase>
			<phrase strongs="7837">early.</phrase>
		</verse>
		<verse num="3">
			<phrase strongs="3034">I will praise</phrase>
			<phrase strongs="3068">thee, O LORD,</phrase>
			<phrase strongs="5971">among the people:</phrase>
			<phrase strongs="2167">and I will sing praises</phrase>
			<phrase strongs="3816">unto thee among the nations.</phrase>
		</verse>
		<verse num="4">
			<phrase strongs="2617">For thy mercy</phrase>
			<phrase strongs="1419">is great</phrase>
			<phrase strongs="8064">above the heavens:</phrase>
			<phrase strongs="571">and thy truth</phrase>
			<phrase strongs="7834">reacheth unto the clouds.</phrase>
		</verse>
		<verse num="5">
			<phrase strongs="7311">Be thou exalted,</phrase>
			<phrase strongs="430">O God,</phrase>
			<phrase strongs="8064">above the heavens:</phrase>
			<phrase strongs="3519">and thy glory</phrase>
			<phrase strongs="776">above all the earth;</phrase>
		</verse>
		<verse num="6">
			<phrase strongs="3039">That thy beloved</phrase>
			<phrase strongs="2502">may be delivered:</phrase>
			<phrase strongs="3467">save</phrase>
			<phrase strongs="3225">with thy right hand,</phrase>
			<phrase strongs="6030">and answerme.</phrase>
		</verse>
		<verse num="7">
			<phrase strongs="430">God</phrase>
			<phrase strongs="1696">hath spoken</phrase>
			<phrase strongs="6944">in his holiness;</phrase>
			<phrase strongs="5937">I will rejoice,</phrase>
			<phrase strongs="2505">I will divide</phrase>
			<phrase strongs="7927">Shechem,</phrase>
			<phrase strongs="4058">and mete out</phrase>
			<phrase strongs="6010">the valley</phrase>
			<phrase strongs="5523">of Succoth.</phrase>
		</verse>
		<verse num="8">
			<phrase strongs="1568">Gilead</phrase>
			<phrase strongs="4519">is mine; Manasseh</phrase>
			<phrase strongs="669">is mine; Ephraim</phrase>
			<phrase strongs="4581">also is the strength</phrase>
			<phrase strongs="7218">of mine head;</phrase>
			<phrase strongs="3063">Judah</phrase>
			<phrase strongs="2710">is my lawgiver;</phrase>
		</verse>
		<verse num="9">
			<phrase strongs="4124">Moab</phrase>
			<phrase strongs="7366">is my washpot</phrase>
			<phrase strongs="5518">;</phrase>
			<phrase strongs="123">over Edom</phrase>
			<phrase strongs="7993">will I cast out</phrase>
			<phrase strongs="5275">my shoe;</phrase>
			<phrase strongs="6429">over Philistia</phrase>
			<phrase strongs="7321">will I triumph.</phrase>
		</verse>
		<verse num="10">
			<phrase strongs="2986">Who will bring</phrase>
			<phrase strongs="4013">me into the strong</phrase>
			<phrase strongs="5892">city?</phrase>
			<phrase strongs="5148">who will lead</phrase>
			<phrase strongs="123">me into Edom?</phrase>
		</verse>
		<verse num="11">
			<phrase strongs="430">Wilt not thou, O God,</phrase>
			<phrase strongs="2186">who hast cast us off?</phrase>
			<phrase strongs="430">and wilt not thou, O God,</phrase>
			<phrase strongs="3318">go forth</phrase>
			<phrase strongs="6635">with our hosts?</phrase>
		</verse>
		<verse num="12">
			<phrase strongs="3051">Give</phrase>
			<phrase strongs="5833">us help</phrase>
			<phrase strongs="6862">from trouble:</phrase>
			<phrase strongs="7723">for vain</phrase>
			<phrase strongs="8668">is the help</phrase>
			<phrase strongs="120">of man.</phrase>
		</verse>
		<verse num="13">
			<phrase strongs="430">Through God</phrase>
			<phrase strongs="6213">we shall do</phrase>
			<phrase strongs="2428">valiantly:</phrase>
			<phrase strongs="947">for he it is that shall tread down</phrase>
			<phrase strongs="6862">our enemies.</phrase>
		</verse>
	</chapter>
