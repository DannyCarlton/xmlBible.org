	<chapter num="19">
		<verse num="1">
			<phrase strongs="5329">To the chief Musician,</phrase>
			<phrase strongs="4210">A Psalm</phrase>
			<phrase strongs="1732">of David.</phrase>
			<phrase strongs="8064">The heavens</phrase>
			<phrase strongs="5608">declare</phrase>
			<phrase strongs="3519">the glory</phrase>
			<phrase strongs="410">of God;</phrase>
