			<phrase strongs="4639">handywork</phrase>
			<phrase strongs="3027">.</phrase>
		</verse>
		<verse num="2">
			<phrase strongs="3117">Day</phrase>
			<phrase strongs="3117">unto day</phrase>
			<phrase strongs="5042">uttereth</phrase>
			<phrase strongs="562">speech,</phrase>
			<phrase strongs="3915">and night</phrase>
