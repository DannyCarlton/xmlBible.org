<book name="Psalms" id="19">
	<chapter num="120">
		<verse num="1">
			<phrase strongs="7892">A Song</phrase>
			<phrase strongs="4609">of degrees.</phrase>
			<phrase strongs="6869">In my distress</phrase>
			<phrase strongs="7121">I cried</phrase>
			<phrase strongs="3068">unto the LORD,</phrase>
			<phrase strongs="6030">and he heardme.</phrase>
		</verse>
		<verse num="2">
			<phrase strongs="5337">Deliver</phrase>
			<phrase strongs="5315">my soul,</phrase>
			<phrase strongs="3068">O LORD,</phrase>
			<phrase strongs="8267">from lying</phrase>
			<phrase strongs="8193">lips,</phrase>
			<phrase strongs="7423">and from a deceitful</phrase>
			<phrase strongs="3956">tongue.</phrase>
		</verse>
		<verse num="3">
			<phrase strongs="5414">What shall be given</phrase>
			<phrase strongs="3254">unto thee? or what shall be done</phrase>
			<phrase strongs="7423">unto thee, thou false</phrase>
			<phrase strongs="3956">tongue?</phrase>
		</verse>
		<verse num="4">
			<phrase strongs="8150">Sharp</phrase>
			<phrase strongs="2671">arrows</phrase>
			<phrase strongs="1368">of the mighty,</phrase>
			<phrase strongs="1513">with coals</phrase>
			<phrase strongs="7574">of juniper.</phrase>
		</verse>
		<verse num="5">
			<phrase strongs="190">Woe</phrase>
			<phrase strongs="1481">is me, that I sojourn</phrase>
			<phrase strongs="4902">in Mesech,</phrase>
			<phrase strongs="7931">that I dwell</phrase>
			<phrase strongs="168">in the tents</phrase>
			<phrase strongs="6938">of Kedar!</phrase>
		</verse>
		<verse num="6">
			<phrase strongs="5315">My soul</phrase>
			<phrase strongs="7227">hath long</phrase>
			<phrase strongs="7931">dwelt</phrase>
			<phrase strongs="8130">with him that hateth</phrase>
			<phrase strongs="7965">peace.</phrase>
		</verse>
		<verse num="7">
			<phrase strongs="7965">I am for peace:</phrase>
			<phrase strongs="1696">but when I speak,</phrase>
			<phrase strongs="4421">they are for war.</phrase>
		</verse>
	</chapter>
</book>
