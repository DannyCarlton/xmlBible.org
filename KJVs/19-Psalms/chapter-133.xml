	<chapter num="133">
		<verse num="1">
			<phrase strongs="7892">A Song</phrase>
			<phrase strongs="4609">of degrees</phrase>
			<phrase strongs="1732">of David.</phrase>
			<phrase strongs="2896">Behold, how good</phrase>
			<phrase strongs="5273">and how pleasant</phrase>
			<phrase strongs="251">it is for brethren</phrase>
			<phrase strongs="3427">to dwell</phrase>
			<phrase strongs="3162">together</phrase>
			<phrase strongs="3162">in unity!</phrase>
		</verse>
		<verse num="2">
			<phrase strongs="2896">It is like the precious</phrase>
			<phrase strongs="8081">ointment</phrase>
			<phrase strongs="7218">upon the head,</phrase>
			<phrase strongs="3381">that ran down</phrase>
			<phrase strongs="2206">upon the beard,</phrase>
			<phrase strongs="175">even Aaron's</phrase>
			<phrase strongs="2206">beard:</phrase>
			<phrase strongs="3381">that went down</phrase>
			<phrase strongs="6310">to the skirts</phrase>
			<phrase strongs="4060">of his garments;</phrase>
		</verse>
		<verse num="3">
			<phrase strongs="2919">As the dew</phrase>
			<phrase strongs="2768">of Hermon,</phrase>
			<phrase strongs="3381">and as the dew that descended</phrase>
			<phrase strongs="2042">upon the mountains</phrase>
			<phrase strongs="6726">of Zion:</phrase>
			<phrase strongs="3068">for there the LORD</phrase>
			<phrase strongs="6680">commanded</phrase>
			<phrase strongs="1293">the blessing,</phrase>
			<phrase strongs="2416">even life</phrase>
			<phrase strongs="5769">for evermore.</phrase>
