<book name="Psalms" id="19">
	<chapter num="16">
		<verse num="1">
			<phrase strongs="4387">Michtam</phrase>
			<phrase strongs="1732">of David.</phrase>
			<phrase strongs="8104">Preserve</phrase>
			<phrase strongs="410">me, O God:</phrase>
			<phrase strongs="2620">for in thee do I put my trust.</phrase>
		</verse>
		<verse num="2">
			<phrase strongs="559">O my soul, thou hast said</phrase>
			<phrase strongs="3068">unto the LORD,</phrase>
			<phrase strongs="136">Thou art my Lord:</phrase>
			<phrase strongs="2896">my goodness extendeth not to thee;</phrase>
		</verse>
		<verse num="3">
			<phrase strongs="6918">But to the saints</phrase>
			<phrase strongs="1992">that</phrase>
			<phrase strongs="776">are in the earth,</phrase>
			<phrase strongs="117">and to the excellent,</phrase>
			<phrase strongs="2656">in whom is all my delight.</phrase>
		</verse>
		<verse num="4">
			<phrase strongs="6094">Their sorrows</phrase>
			<phrase strongs="7235">shall be multiplied</phrase>
			<phrase strongs="4116">that hasten</phrase>
			<phrase strongs="312">after another</phrase>
			<phrase strongs="5262">god: their drink offerings</phrase>
			<phrase strongs="1818">of blood</phrase>
			<phrase strongs="5258">will I not offer,</phrase>
			<phrase strongs="1077">nor</phrase>
			<phrase strongs="5375">take up</phrase>
			<phrase strongs="8034">their names</phrase>
			<phrase strongs="8193">into my lips.</phrase>
		</verse>
		<verse num="5">
			<phrase strongs="3068">The LORD</phrase>
			<phrase strongs="4490">is the portion</phrase>
			<phrase strongs="2506">of mine inheritance</phrase>
			<phrase strongs="3563">and of my cup:</phrase>
			<phrase strongs="8551">thou maintainest</phrase>
			<phrase strongs="1486">my lot.</phrase>
		</verse>
		<verse num="6">
			<phrase strongs="2256">The lines</phrase>
			<phrase strongs="5307">are fallen</phrase>
			<phrase strongs="5273">unto me in pleasant</phrase>
			<phrase strongs="8231">places; yea, I have a goodly</phrase>
			<phrase strongs="5159">heritage.</phrase>
		</verse>
		<verse num="7">
			<phrase strongs="1288">I will bless</phrase>
			<phrase strongs="3068">the LORD,</phrase>
			<phrase strongs="3289">who hath given me counsel:</phrase>
			<phrase strongs="3629">my reins</phrase>
			<phrase strongs="3256">also instruct</phrase>
			<phrase strongs="3915">me in the night seasons.</phrase>
		</verse>
		<verse num="8">
			<phrase strongs="7737">I have set</phrase>
			<phrase strongs="3068">the LORD</phrase>
			<phrase strongs="8548">always</phrase>
			<phrase strongs="3225">before me: because he is at my right hand,</phrase>
			<phrase strongs="4131">I shall not be moved.</phrase>
		</verse>
		<verse num="9">
			<phrase strongs="3820">Therefore my heart</phrase>
			<phrase strongs="8055">is glad,</phrase>
			<phrase strongs="3519">and my glory</phrase>
			<phrase strongs="1523">rejoiceth:</phrase>
			<phrase strongs="1320">my flesh</phrase>
			<phrase strongs="7931">also shall rest</phrase>
			<phrase strongs="983">in hope.</phrase>
		</verse>
		<verse num="10">
			<phrase strongs="5800">For thou wilt not leave</phrase>
			<phrase strongs="5315">my soul</phrase>
			<phrase strongs="7585">in hell;</phrase>
			<phrase strongs="5414">neither wilt thou suffer</phrase>
			<phrase strongs="2623">thine Holy One</phrase>
			<phrase strongs="7200">to see</phrase>
			<phrase strongs="7845">corruption.</phrase>
		</verse>
		<verse num="11">
			<phrase strongs="3045">Thou wilt shew</phrase>
			<phrase strongs="734">me the path</phrase>
			<phrase strongs="2416">of life:</phrase>
			<phrase strongs="6440">in thy presence</phrase>
			<phrase strongs="7648">is fulness</phrase>
			<phrase strongs="8057">of joy;</phrase>
			<phrase strongs="3225">at thy right hand</phrase>
			<phrase strongs="5273">there are pleasures</phrase>
			<phrase strongs="5331">for evermore.</phrase>
		</verse>
	</chapter>
</book>
