			<phrase strongs="3068">me, O LORD,</phrase>
			<phrase strongs="3027">from the hands</phrase>
			<phrase strongs="7563">of the wicked;</phrase>
			<phrase strongs="5341">preserve</phrase>
			<phrase strongs="2555">me from the violent</phrase>
			<phrase strongs="376">man;</phrase>
			<phrase strongs="2803">who have purposed</phrase>
			<phrase strongs="1760">to overthrow</phrase>
			<phrase strongs="6471">my goings.</phrase>
		</verse>
		<verse num="5">
			<phrase strongs="1343">The proud</phrase>
			<phrase strongs="2934">have hid</phrase>
			<phrase strongs="6341">a snare</phrase>
			<phrase strongs="2256">for me, and cords;</phrase>
			<phrase strongs="6566">they have spread</phrase>
