			<phrase strongs="4170">gins</phrase>
			<phrase strongs="5542">for me. Selah.</phrase>
		</verse>
		<verse num="6">
			<phrase strongs="559">I said</phrase>
			<phrase strongs="3068">unto the LORD,</phrase>
			<phrase strongs="410">Thou art my God:</phrase>
			<phrase strongs="238">hear</phrase>
			<phrase strongs="6963">the voice</phrase>
			<phrase strongs="8469">of my supplications,</phrase>
			<phrase strongs="3068">O LORD.</phrase>
		</verse>
		<verse num="7">
			<phrase strongs="3069">O GOD</phrase>
