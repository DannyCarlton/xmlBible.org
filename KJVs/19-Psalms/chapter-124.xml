	<chapter num="124">
		<verse num="1">
			<phrase strongs="7892">A Song</phrase>
			<phrase strongs="4609">of degrees</phrase>
			<phrase strongs="1732">of David.</phrase>
			<phrase strongs="3884">If</phrase>
			<phrase strongs="3068">it had not been the LORD</phrase>
			<phrase strongs="3478">who was on our side, now may Israel</phrase>
			<phrase strongs="559">say;</phrase>
		</verse>
		<verse num="2">
			<phrase strongs="3884">If</phrase>
			<phrase strongs="3068">it had not been the LORD</phrase>
			<phrase strongs="120">who was on our side, when men</phrase>
			<phrase strongs="6965">rose upagainst us:</phrase>
		</verse>
		<verse num="3">
			<phrase strongs="233">Then</phrase>
			<phrase strongs="1104">they had swallowed us up</phrase>
			<phrase strongs="2416">quick,</phrase>
			<phrase strongs="639">when their wrath</phrase>
			<phrase strongs="2734">was kindledagainst us:</phrase>
		</verse>
		<verse num="4">
			<phrase strongs="233">Then</phrase>
			<phrase strongs="4325">the waters</phrase>
			<phrase strongs="7857">had overwhelmed</phrase>
			<phrase strongs="5158">us, the stream</phrase>
			<phrase strongs="5674">had gone over</phrase>
			<phrase strongs="5315">our soul:</phrase>
		</verse>
		<verse num="5">
			<phrase strongs="233">Then</phrase>
			<phrase strongs="2121">the proud</phrase>
			<phrase strongs="4325">waters</phrase>
			<phrase strongs="5674">had gone over</phrase>
			<phrase strongs="5315">our soul.</phrase>
		</verse>
		<verse num="6">
			<phrase strongs="1288">Blessed</phrase>
			<phrase strongs="3068">be the LORD,</phrase>
			<phrase strongs="5414">who hath not given</phrase>
			<phrase strongs="2964">us as a prey</phrase>
			<phrase strongs="8127">to their teeth.</phrase>
		</verse>
		<verse num="7">
			<phrase strongs="5315">Our soul</phrase>
			<phrase strongs="4422">is escaped</phrase>
			<phrase strongs="6833">as a bird</phrase>
			<phrase strongs="6341">out of the snare</phrase>
			<phrase strongs="3369">of the fowlers:</phrase>
			<phrase strongs="6341">the snare</phrase>
			<phrase strongs="7665">is broken,</phrase>
			<phrase strongs="4422">and we are escaped.</phrase>
		</verse>
		<verse num="8">
			<phrase strongs="5828">Our help</phrase>
			<phrase strongs="8034">is in the name</phrase>
			<phrase strongs="3068">of the LORD,</phrase>
			<phrase strongs="6213">who made</phrase>
			<phrase strongs="8064">heaven</phrase>
			<phrase strongs="776">and earth.</phrase>
