			<phrase strongs="6779">to bud:</phrase>
			<phrase strongs="6186">I have ordained</phrase>
			<phrase strongs="5216">a lamp</phrase>
			<phrase strongs="4899">for mine anointed.</phrase>
		</verse>
		<verse num="18">
			<phrase strongs="341">His enemies</phrase>
			<phrase strongs="3847">will I clothe</phrase>
			<phrase strongs="1322">with shame:</phrase>
