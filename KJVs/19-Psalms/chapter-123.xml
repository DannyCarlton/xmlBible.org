<book name="Psalms" id="19">
	<chapter num="123">
		<verse num="1">
			<phrase strongs="7892">A Song</phrase>
			<phrase strongs="4609">of degrees.</phrase>
			<phrase strongs="5375">Unto thee lift I up</phrase>
			<phrase strongs="5869">mine eyes,</phrase>
			<phrase strongs="3427">O thou that dwellest</phrase>
			<phrase strongs="8064">in the heavens.</phrase>
		</verse>
		<verse num="2">
			<phrase strongs="5869">Behold, as the eyes</phrase>
			<phrase strongs="5650">of servants</phrase>
			<phrase strongs="3027">look unto the hand</phrase>
			<phrase strongs="113">of their masters,</phrase>
			<phrase strongs="5869">and as the eyes</phrase>
			<phrase strongs="8198">of a maiden</phrase>
			<phrase strongs="3027">unto the hand</phrase>
			<phrase strongs="1404">of her mistress;</phrase>
			<phrase strongs="5869">so our eyes</phrase>
			<phrase strongs="3068">wait upon the LORD</phrase>
			<phrase strongs="430">our God,</phrase>
			<phrase strongs="2603">until that he have mercyupon us.</phrase>
		</verse>
		<verse num="3">
			<phrase strongs="2603">Have mercy</phrase>
			<phrase strongs="3068">upon us, O LORD,</phrase>
			<phrase strongs="2603">have mercy</phrase>
			<phrase strongs="7227">upon us: for we are exceedingly</phrase>
			<phrase strongs="7646">filled</phrase>
			<phrase strongs="937">with contempt.</phrase>
		</verse>
		<verse num="4">
			<phrase strongs="5315">Our soul</phrase>
			<phrase strongs="7227">is exceedingly</phrase>
			<phrase strongs="7646">filled</phrase>
			<phrase strongs="3933">with the scorning</phrase>
			<phrase strongs="7600">of those that are at ease,</phrase>
			<phrase strongs="937">and with the contempt</phrase>
			<phrase strongs="3238">of the proud</phrase>
			<phrase strongs="1343"></phrase>
			<phrase strongs="1349">.</phrase>
		</verse>
