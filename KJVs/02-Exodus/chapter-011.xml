			<phrase strongs="3068">And the LORD</phrase>
			<phrase strongs="5414">gave</phrase>
			<phrase strongs="5971">the people</phrase>
			<phrase strongs="2580">favour</phrase>
			<phrase strongs="5869">in the sight</phrase>
			<phrase strongs="4714">of the Egyptians.</phrase>
			<phrase strongs="376">Moreover the man</phrase>
			<phrase strongs="4872">Moses</phrase>
			<phrase strongs="3966">was very</phrase>
			<phrase strongs="1419">great</phrase>
			<phrase strongs="776">in the land</phrase>
			<phrase strongs="4714">of Egypt,</phrase>
			<phrase strongs="5869">in the sight</phrase>
			<phrase strongs="6547">of Pharaoh's</phrase>
			<phrase strongs="5650">servants,</phrase>
			<phrase strongs="5869">and in the sight</phrase>
			<phrase strongs="5971">of the people.</phrase>
		</verse>
		<verse num="4">
			<phrase strongs="4872">And Moses</phrase>
			<phrase strongs="559">said,</phrase>
			<phrase strongs="559">Thus saith</phrase>
			<phrase strongs="3068">the LORD,</phrase>
			<phrase strongs="3915">About midnight</phrase>
			<phrase strongs="2676"></phrase>
			<phrase strongs="3318">will I go out</phrase>
			<phrase strongs="8432">into the midst</phrase>
			<phrase strongs="4714">of Egypt:</phrase>
		</verse>
