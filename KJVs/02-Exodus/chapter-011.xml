		<verse num="1">
			<phrase strongs="3068">And the LORD</phrase>
			<phrase strongs="559">said</phrase>
			<phrase strongs="4872">unto Moses,</phrase>
			<phrase strongs="935">Yet will I bring</phrase>
			<phrase strongs="259">one</phrase>
			<phrase strongs="5061">plague</phrase>
			<phrase strongs="6547">more upon Pharaoh,</phrase>
			<phrase strongs="4714">and upon Egypt;</phrase>
			<phrase strongs="310">afterwards</phrase>
			<phrase strongs="7971">he will let you go</phrase>
			<phrase strongs="7971">hence: when he shall let you go,</phrase>
			<phrase strongs="1644">he shall surely</phrase>
			<phrase strongs="1644">thrust you out</phrase>
			<phrase strongs="3617">hence altogether.</phrase>
		</verse>
		<verse num="2">
			<phrase strongs="1696">Speak</phrase>
			<phrase strongs="241">now in the ears</phrase>
			<phrase strongs="5971">of the people,</phrase>
