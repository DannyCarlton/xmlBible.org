			<phrase strongs="559">them, saying,</phrase>
			<phrase strongs="3615">Fulfil</phrase>
			<phrase strongs="4639">your works,</phrase>
			<phrase strongs="3117">your daily</phrase>
			<phrase strongs="1697">tasks,</phrase>
			<phrase strongs="1961">as when there was</phrase>
			<phrase strongs="8401">straw.</phrase>
		</verse>
		<verse num="14">
			<phrase strongs="7860">And the officers</phrase>
			<phrase strongs="1121">of the children</phrase>
			<phrase strongs="3478">of Israel,</phrase>
