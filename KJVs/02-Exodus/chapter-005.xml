			<phrase strongs="6547">Pharaoh,</phrase>
			<phrase strongs="5414">I will not give</phrase>
			<phrase strongs="8401">you straw.</phrase>
		</verse>
		<verse num="11">
			<phrase strongs="3212">Go</phrase>
			<phrase strongs="3947">ye, get</phrase>
			<phrase strongs="8401">you straw</phrase>
			<phrase strongs="834">where</phrase>
			<phrase strongs="4672">ye can find</phrase>
			<phrase strongs="3588">it: yet</phrase>
			<phrase strongs="1697">not ought</phrase>
			<phrase strongs="5656">of your work</phrase>
			<phrase strongs="1639">shall be diminished.</phrase>
		</verse>
