			<phrase strongs="6440">before</phrase>
			<phrase strongs="251">your brethren</phrase>
			<phrase strongs="1121">the children</phrase>
			<phrase strongs="3478">of Israel,</phrase>
			<phrase strongs="1121">all that are meet</phrase>
			<phrase strongs="2428">for the war.</phrase>
		</verse>
		<verse num="19">
