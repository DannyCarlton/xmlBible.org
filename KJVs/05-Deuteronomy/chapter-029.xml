			<phrase strongs="7579">unto the drawer</phrase>
			<phrase strongs="4325">of thy water:</phrase>
		</verse>
		<verse num="12">
			<phrase strongs="5674">That thou shouldest enter</phrase>
			<phrase strongs="1285">into covenant</phrase>
			<phrase strongs="3068">with the LORD</phrase>
