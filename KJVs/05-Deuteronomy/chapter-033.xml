			<phrase strongs="6201">shall drop down</phrase>
			<phrase strongs="2919">dew.</phrase>
		</verse>
		<verse num="29">
			<phrase strongs="835">Happy</phrase>
			<phrase strongs="3478">art thou, O Israel:</phrase>
			<phrase strongs="5971">who is like unto thee, O people</phrase>
			<phrase strongs="3467">saved</phrase>
