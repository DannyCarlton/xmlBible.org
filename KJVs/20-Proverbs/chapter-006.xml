		<verse num="18">
			<phrase strongs="3820">An heart</phrase>
			<phrase strongs="2790">that deviseth</phrase>
			<phrase strongs="205">wicked</phrase>
			<phrase strongs="4284">imaginations,</phrase>
			<phrase strongs="7272">feet</phrase>
			<phrase strongs="4116">that be swift</phrase>
			<phrase strongs="7323">in running</phrase>
