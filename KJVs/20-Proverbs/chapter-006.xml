			<phrase strongs="6597">suddenly;</phrase>
			<phrase strongs="6621">suddenly</phrase>
			<phrase strongs="7665">shall he be broken</phrase>
			<phrase strongs="4832">without remedy.</phrase>
		</verse>
		<verse num="16">
			<phrase strongs="8337">These six</phrase>
			<phrase strongs="3068">things doth the LORD</phrase>
			<phrase strongs="8130">hate:</phrase>
			<phrase strongs="7651">yea, seven</phrase>
			<phrase strongs="8441">are an abomination</phrase>
