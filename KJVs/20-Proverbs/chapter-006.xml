		<verse num="11">
			<phrase strongs="7389">So shall thy poverty</phrase>
			<phrase strongs="935">come</phrase>
			<phrase strongs="1980">as one that travelleth,</phrase>
			<phrase strongs="4270">and thy want</phrase>
			<phrase strongs="4043">as an armed</phrase>
			<phrase strongs="376">man.</phrase>
		</verse>
