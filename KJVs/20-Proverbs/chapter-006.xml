			<phrase strongs="2790">he deviseth</phrase>
			<phrase strongs="7451">mischief</phrase>
			<phrase strongs="6256">continually;</phrase>
			<phrase strongs="7971">he soweth</phrase>
			<phrase strongs="4066">discord</phrase>
			<phrase strongs="4090">.</phrase>
		</verse>
		<verse num="15">
			<phrase strongs="343">Therefore shall his calamity</phrase>
