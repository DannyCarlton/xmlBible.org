			<phrase strongs="802">wife;</phrase>
			<phrase strongs="5060">whosoever toucheth</phrase>
			<phrase strongs="5352">her shall not be innocent.</phrase>
		</verse>
		<verse num="30">
			<phrase strongs="936">Men do not despise</phrase>
			<phrase strongs="1590">a thief,</phrase>
			<phrase strongs="1589">if he steal</phrase>
			<phrase strongs="4390">to satisfy</phrase>
			<phrase strongs="5315">his soul</phrase>
			<phrase strongs="7456">when he is hungry;</phrase>
		</verse>
		<verse num="31">
			<phrase strongs="4672">But if he be found,</phrase>
