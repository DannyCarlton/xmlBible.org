			<phrase strongs="6310">mouth.</phrase>
		</verse>
		<verse num="13">
			<phrase strongs="7169">He winketh</phrase>
			<phrase strongs="5869">with his eyes,</phrase>
			<phrase strongs="4448">he speaketh</phrase>
			<phrase strongs="7272">with his feet,</phrase>
			<phrase strongs="3384">he teacheth</phrase>
