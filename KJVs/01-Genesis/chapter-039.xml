		</verse>
		<verse num="8">
			<phrase strongs="3985">But he refused,</phrase>
			<phrase strongs="559">and said</phrase>
			<phrase strongs="113">unto his master's</phrase>
			<phrase strongs="802">wife,</phrase>
			<phrase strongs="113">Behold my master</phrase>
			<phrase strongs="3045">wotteth</phrase>
