			<phrase strongs="5414">and he hath committed</phrase>
			<phrase strongs="3426">all that he hath</phrase>
			<phrase strongs="3027">to my hand;</phrase>
		</verse>
		<verse num="9">
			<phrase strongs="1419">There is none greater</phrase>
			<phrase strongs="1004">in this house</phrase>
			<phrase strongs="3808">than I; neither</phrase>
			<phrase strongs="2820">hath he kept back</phrase>
			<phrase strongs="3972">any thing</phrase>
