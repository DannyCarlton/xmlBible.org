			<phrase strongs="5291">unto the damsel.</phrase>
		</verse>
		<verse num="4">
			<phrase strongs="7927">And Shechem</phrase>
			<phrase strongs="559">spake</phrase>
			<phrase strongs="1">unto his father</phrase>
			<phrase strongs="2544">Hamor,</phrase>
			<phrase strongs="559">saying,</phrase>
			<phrase strongs="3947">Get</phrase>
			<phrase strongs="3207">me this damsel</phrase>
			<phrase strongs="802">to wife.</phrase>
		</verse>
		<verse num="5">
			<phrase strongs="3290">And Jacob</phrase>
