			<phrase strongs="4735">were with his cattle</phrase>
			<phrase strongs="7704">in the field:</phrase>
			<phrase strongs="3290">and Jacob</phrase>
			<phrase strongs="2790">held his peace</phrase>
			<phrase strongs="935">until they were come.</phrase>
		</verse>
		<verse num="6">
			<phrase strongs="2544">And Hamor</phrase>
			<phrase strongs="1">the father</phrase>
			<phrase strongs="7927">of Shechem</phrase>
			<phrase strongs="3318">went out</phrase>
			<phrase strongs="3290">unto Jacob</phrase>
			<phrase strongs="1696">to commune</phrase>
			<phrase strongs="854">with him.</phrase>
		</verse>
		<verse num="7">
			<phrase strongs="1121">And the sons</phrase>
			<phrase strongs="3290">of Jacob</phrase>
			<phrase strongs="935">came</phrase>
			<phrase strongs="7704">out of the field</phrase>
			<phrase strongs="8085">when they heard</phrase>
			<phrase strongs="582">it: and the men</phrase>
			<phrase strongs="6087">were grieved,</phrase>
			<phrase strongs="3966">and they were very</phrase>
			<phrase strongs="2734">wroth,</phrase>
			<phrase strongs="6213">because he had wrought</phrase>
			<phrase strongs="5039">folly</phrase>
			<phrase strongs="3478">in Israel</phrase>
			<phrase strongs="7901">in lying</phrase>
			<phrase strongs="3290">with Jacob's</phrase>
