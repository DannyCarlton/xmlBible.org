		</verse>
		<verse num="3">
			<phrase strongs="5315">And his soul</phrase>
			<phrase strongs="1692">clave</phrase>
			<phrase strongs="1783">unto Dinah</phrase>
			<phrase strongs="1323">the daughter</phrase>
			<phrase strongs="3290">of Jacob,</phrase>
