			<phrase strongs="8299">branches</phrase>
			<phrase strongs="7969">are three</phrase>
			<phrase strongs="3117">days:</phrase>
		</verse>
		<verse num="13">
			<phrase strongs="5750">Yet</phrase>
			<phrase strongs="7969">within three</phrase>
			<phrase strongs="3117">days</phrase>
			<phrase strongs="6547">shall Pharaoh</phrase>
			<phrase strongs="5375">lift up</phrase>
			<phrase strongs="7218">thine head,</phrase>
			<phrase strongs="7725">and restore</phrase>
			<phrase strongs="3653">thee unto thy place:</phrase>
