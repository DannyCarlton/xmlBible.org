			<phrase strongs="7969">and behold I had three</phrase>
			<phrase strongs="2751">white</phrase>
			<phrase strongs="5536">baskets</phrase>
			<phrase strongs="7218">on my head:</phrase>
		</verse>
		<verse num="17">
			<phrase strongs="5945">And in the uppermost</phrase>
			<phrase strongs="5536">basket</phrase>
