		<verse num="6">
			<phrase strongs="430">And God</phrase>
			<phrase strongs="559">said</phrase>
			<phrase strongs="2472">unto him in a dream,</phrase>
			<phrase strongs="1571">Yea,</phrase>
			<phrase strongs="3045">I know</phrase>
