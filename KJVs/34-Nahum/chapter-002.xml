			<phrase strongs="2964">with prey,</phrase>
			<phrase strongs="4585">and his dens</phrase>
			<phrase strongs="2966">with ravin.</phrase>
		</verse>
		<verse num="13">
			<phrase strongs="5002">Behold, I am against thee, saith</phrase>
			<phrase strongs="3068">the LORD</phrase>
			<phrase strongs="6635">of hosts,</phrase>
			<phrase strongs="1197">and I will burn</phrase>
