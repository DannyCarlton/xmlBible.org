			<phrase strongs="1245">shall I seek</phrase>
			<phrase strongs="5162">comfortersfor thee?</phrase>
		</verse>
		<verse num="8">
			<phrase strongs="3190">Art thou better</phrase>
			<phrase strongs="527">than populous</phrase>
			<phrase strongs="528"></phrase>
