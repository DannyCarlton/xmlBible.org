			<phrase strongs="1419">and all her great men</phrase>
			<phrase strongs="7576">were bound</phrase>
			<phrase strongs="2131">in chains.</phrase>
		</verse>
		<verse num="11">
			<phrase strongs="7937">Thou also shalt be drunken:</phrase>
			<phrase strongs="5956">thou shalt be hid,</phrase>
			<phrase strongs="1245">thou also shalt seek</phrase>
			<phrase strongs="4581">strength</phrase>
			<phrase strongs="341">because of the enemy.</phrase>
		</verse>
		<verse num="12">
			<phrase strongs="4013">All thy strong holds</phrase>
			<phrase strongs="8384">shall be like fig trees</phrase>
